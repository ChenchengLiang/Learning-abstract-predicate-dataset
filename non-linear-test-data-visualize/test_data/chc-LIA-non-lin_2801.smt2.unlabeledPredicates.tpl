(templates addition@.split ((x0 Int) (x1 Int) (x2 Int))
  (term x0 1)
  (term x1 1)
  (term x2 1)
  (term (+ x0 (* (- 1) x1)) 1)
  (term (+ x0 x1) 1)
  (term (+ x0 (* (- 1) x2)) 1)
  (term (+ x0 x2) 1)
  (inequality-term x0 1)
  (inequality-term x1 1)
  (inequality-term x2 1)
  (inequality-term (* (- 1) x0) 1)
  (inequality-term (* (- 1) x1) 1)
  (inequality-term (* (- 1) x2) 1)
  (inequality-term (+ x0 (* (- 1) x1)) 1)
  (inequality-term (+ x1 (* (- 1) x0)) 1)
  (inequality-term (+ x0 x1) 1)
  (inequality-term (+ (* (- 1) x0) (* (- 1) x1)) 1)
  (inequality-term (+ x0 (* (- 1) x2)) 1)
  (inequality-term (+ x2 (* (- 1) x0)) 1)
  (inequality-term (+ x0 x2) 1)
  (inequality-term (+ (* (- 1) x0) (* (- 1) x2)) 1)
)
